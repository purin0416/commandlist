<%= form_with(model: command) do |f| %>
  <% if command.errors.any? %>
    <div id="error_explanation">
      <ul>
        <% command.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.label :name, 'コマンド' %>
  <%= f.text_field :name %>
  
  <%= f.label :language, '言語' %>
  <%= f.text_field :language %>
  
  <%= f.label :content, '内容' %>
  <%= f.text_field :content %>

  <%= f.submit '追加' %>
<% end %>
